<%- include ("../partials/header.ejs" ) %>

<div class="text-center">
<h2> Car Names</h2>

<h3>Car info</h3>


  <%for (const vehicleName of vehicle ) { %>
        
    <div class="tile">
        <div class="tileHeader">
            <h3><%=vehicleName.make_name%> <%=vehicleName.model_name%></h3>
        </div>
        <div class="link">
            <form method="POST" action="/cars">
                <input hidden id="name" type="text" name="carname" value="<%=vehicleName.make_name%> <%=vehicleName.model_name%>">
                <input hidden id="vehicleId" type="text" name="vehicleId" value="<%=vehicleName.id%>">
                <button type="submit"> Add to Cart</button>
            </form>
        </div>
    </div>
    <% } %>
</div>
   
  <%- include ("../partials/footer.ejs") %> 
